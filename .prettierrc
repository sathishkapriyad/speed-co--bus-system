{
	"trailingComma": "none",
	"singleQuote": true,
	"semi": true,
	"bracketSpacing": true,
	"useTabs": true,
	"tabWidth": 4,
	"arrowParens": "always",
	"jsxSingleQuote": true,
	"jsxBracketSameLine": true,
	"printWidth": 100,
	"quoteProps": "consistent",
	"importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
	"importOrderSeparation": false
}
